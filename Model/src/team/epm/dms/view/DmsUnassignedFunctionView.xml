<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="DmsUnassignedFunctionView"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false"
  OrderBy="SEQ">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="team.epm.dms.view.DmsUnassignedFunctionView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="locale"
    Kind="where"
    Type="java.lang.String">
    <TransientExpression><![CDATA[adf.context.locale]]></TransientExpression>
  </Variable>
  <Variable
    Name="roleId"
    Kind="where"
    Type="java.lang.String"/>
  <ViewAccessor
    Name="LST_USER"
    ViewObjectName="team.epm.dms.view.DmsUserView"
    RowLevelBinds="true"/>
  <ListBinding
    Name="LOV_UpdatedBy"
    ListVOName="LST_USER"
    ListRangeSize="-1"
    NullValueFlag="start"
    NullValueId="${adfBundle['oracle.javatools.resourcebundle.SystemBundle']['NULL_VALUE_RESID']}"
    MRUCount="0">
    <AttrArray Name="AttrNames">
      <Item Value="UpdatedBy"/>
    </AttrArray>
    <AttrArray Name="ListAttrNames">
      <Item Value="Id"/>
    </AttrArray>
    <AttrArray Name="ListDisplayAttrNames">
      <Item Value="Name"/>
    </AttrArray>
    <DisplayCriteria/>
  </ListBinding>
  <SQLQuery>
    <![CDATA[SELECT T.ACTION, 
       T.CATEGORY, 
       T.CREATED_AT, 
       T.CREATED_BY, 
       T.DESCRIPTION, 
       T.ID, 
       T.LOCALE, 
       T.NAME, 
       T.P_FUNCTION_ID, 
       T.PARAMETERS, 
       T.SEQ, 
       T.UPDATED_AT, 
       T.UPDATED_BY
  FROM DMS_FUNCTION T
 WHERE T.LOCALE = :locale
   AND NOT EXISTS (SELECT 1
          FROM DMS_ROLE_FUNCTION F
         WHERE F.FUNCTION_ID = T.ID
           AND F.ROLE_ID = :roleId)]]>
  </SQLQuery>
  <EntityUsage
    Name="DmsFunction"
    Entity="team.epm.dms.model.DmsFunction"/>
  <ViewAttribute
    Name="Action"
    PrecisionRule="true"
    EntityAttrName="Action"
    EntityUsage="DmsFunction"
    AliasName="ACTION"/>
  <ViewAttribute
    Name="Category"
    PrecisionRule="true"
    EntityAttrName="Category"
    EntityUsage="DmsFunction"
    AliasName="CATEGORY"/>
  <ViewAttribute
    Name="CreatedAt"
    IsUpdateable="while_insert"
    PrecisionRule="true"
    EntityAttrName="CreatedAt"
    EntityUsage="DmsFunction"
    AliasName="CREATED_AT"/>
  <ViewAttribute
    Name="CreatedBy"
    PrecisionRule="true"
    EntityAttrName="CreatedBy"
    EntityUsage="DmsFunction"
    AliasName="CREATED_BY"/>
  <ViewAttribute
    Name="Description"
    PrecisionRule="true"
    EntityAttrName="Description"
    EntityUsage="DmsFunction"
    AliasName="DESCRIPTION"/>
  <ViewAttribute
    Name="Id"
    IsUpdateable="while_insert"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Id"
    EntityUsage="DmsFunction"
    AliasName="ID"/>
  <ViewAttribute
    Name="Locale"
    IsUpdateable="while_insert"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Locale"
    EntityUsage="DmsFunction"
    AliasName="LOCALE"/>
  <ViewAttribute
    Name="Name"
    PrecisionRule="true"
    EntityAttrName="Name"
    EntityUsage="DmsFunction"
    AliasName="NAME"/>
  <ViewAttribute
    Name="PFunctionId"
    PrecisionRule="true"
    EntityAttrName="PFunctionId"
    EntityUsage="DmsFunction"
    AliasName="P_FUNCTION_ID"/>
  <ViewAttribute
    Name="Parameters"
    PrecisionRule="true"
    EntityAttrName="Parameters"
    EntityUsage="DmsFunction"
    AliasName="PARAMETERS"/>
  <ViewAttribute
    Name="Seq"
    PrecisionRule="true"
    EntityAttrName="Seq"
    EntityUsage="DmsFunction"
    AliasName="SEQ"/>
  <ViewAttribute
    Name="UpdatedAt"
    PrecisionRule="true"
    EntityAttrName="UpdatedAt"
    EntityUsage="DmsFunction"
    AliasName="UPDATED_AT"/>
  <ViewAttribute
    Name="UpdatedBy"
    PrecisionRule="true"
    EntityAttrName="UpdatedBy"
    EntityUsage="DmsFunction"
    AliasName="UPDATED_BY"
    LOVName="LOV_UpdatedBy">
    <Properties>
      <SchemaBasedProperties>
        <CONTROLTYPE
          Value="choice"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="ModelBundle"/>
  </ResourceBundle>
</ViewObject>

    EntityUsage="DcmTemplate"
    AliasName="LOCALE"/>
  <ViewAttribute
    Name="Name"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Name"
    EntityUsage="DcmTemplate"
    AliasName="NAME">
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="dcm.template.name"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="PreProgram"
    PrecisionRule="true"
    EntityAttrName="PreProgram"
    EntityUsage="DcmTemplate"
    AliasName="PRE_PROGRAM"/>
  <ViewAttribute
    Name="Readonly"
    PrecisionRule="true"

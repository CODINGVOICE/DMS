  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false"
  OrderBy="SEQ">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="team.epm.dcm.view.DcmUnAssignedTemplate_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="locale"
    Kind="where"
    Type="java.lang.String">
    <TransientExpression><![CDATA[adf.context.locale]]></TransientExpression>
  </Variable>
  <Variable

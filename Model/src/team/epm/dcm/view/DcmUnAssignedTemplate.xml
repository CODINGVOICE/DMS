    EntityUsage="DcmTemplate"
    AliasName="SEQ"/>
  <ViewAttribute
    Name="TemplateFile"
    PrecisionRule="true"
    EntityAttrName="TemplateFile"
    EntityUsage="DcmTemplate"
    AliasName="TEMPLATE_FILE"/>
  <ViewAttribute
    Name="TmpTable"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="TmpTable"
    EntityUsage="DcmTemplate"
    AliasName="TMP_TABLE"/>

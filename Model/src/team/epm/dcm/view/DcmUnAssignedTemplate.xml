      <SchemaBasedProperties>
        <LABEL
          ResId="updatedAt"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="UpdatedBy"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="UpdatedBy"
    EntityUsage="DcmTemplate"
    AliasName="UPDATED_BY"
    LOVName="LOV_UpdatedBy">
    <Properties>
      <SchemaBasedProperties>
        <CONTROLTYPE
          Value="choice"/>
        <LABEL
          ResId="updatedBy"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>

  <ViewAccessor
    Name="LST_USER"
    ViewObjectName="team.epm.dms.view.DmsUserView"
    RowLevelBinds="true"/>
  <ListBinding
    Name="LOV_UpdatedBy"
    ListVOName="LST_USER"
    ListRangeSize="-1"
    NullValueFlag="start"
    NullValueId="${adfBundle['oracle.javatools.resourcebundle.SystemBundle']['NULL_VALUE_RESID']}"
    MRUCount="0">
    <AttrArray Name="AttrNames">
      <Item Value="UpdatedBy"/>
    </AttrArray>
    <AttrArray Name="ListAttrNames">
      <Item Value="Id"/>
    </AttrArray>
    <AttrArray Name="ListDisplayAttrNames">
      <Item Value="Name"/>
    </AttrArray>
    <DisplayCriteria/>
  </ListBinding>
  <SQLQuery>
    <![CDATA[SELECT DcmTemplate.AFTER_PROGRAM, 
       DcmTemplate.CATEGORY_ID, 
       DcmTemplate.COMBINATION_ID, 
       DcmTemplate.CREATED_AT, 
       DcmTemplate.CREATED_BY, 
       DcmTemplate.DATA_START_LINE, 
       DcmTemplate.DB_TABLE, 
       DcmTemplate.DB_VIEW, 
       DcmTemplate.DESCRIPTION, 
       DcmTemplate.HANDLE_MODE, 
       DcmTemplate.HANDLE_PROGRAM, 
       DcmTemplate.ID, 
       DcmTemplate.LOCALE, 
       DcmTemplate.NAME, 
       DcmTemplate.PRE_PROGRAM, 
       DcmTemplate.READONLY, 
       DcmTemplate.SEQ, 
       DcmTemplate.TEMPLATE_FILE, 
       DcmTemplate.TMP_TABLE, 
       DcmTemplate.UPDATED_AT, 
       DcmTemplate.UPDATED_BY
FROM DCM_TEMPLATE DcmTemplate
WHERE DcmTemplate.Locale=:locale 
       AND NOT EXISTS (SELECT 1 
                       FROM DCM_ROLE_TEMPLATE T 
                       WHERE T.ROLE_ID=:roleId 
                       AND T.TEMPLATE_ID=DcmTemplate.Id)]]>
  </SQLQuery>
